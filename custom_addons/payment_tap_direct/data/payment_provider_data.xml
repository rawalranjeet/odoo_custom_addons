<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_tap_direct" model="payment.provider">
        <field name="name">Tap Direct</field>
        <field name="code">tap</field>
        <field name="state">disabled</field>
        <field name="inline_form_view_id" ref="payment_tap_direct.inline_form"/>
        <field name="image_128" type="base64" file="payment_tap_direct/static/img/tap.png"/>
        <field name="module_id" ref="base.module_payment_tap_direct"/>
        <field name="payment_method_ids"
               eval="[Command.set([
                        ref('payment.payment_method_card'),
                        ref('payment_tap_direct.payment_method_tap_direct')
                     ])]"
        />
    </record>

</odoo>

