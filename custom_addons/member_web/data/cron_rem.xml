<odoo>
    <data noupdate="1">
        <record id="ir_cron_membership_expiry_reminder" model="ir.cron">
            <field name="name">Membership Expiry Reminder</field>
            <field name="model_id" ref="model_membership_membership"/>
            <field name="state">code</field>
            <field name="code">model._send_expiry_reminders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
